                        <h1 class="mt-4">Create new Author</h1>
                       
                      
                            <div class="row mt-4">
                                <form enctype="multipart/form-data" method="post">

                                    <div class="form-group my-3">
                                      <label for="name">Name</label>
                                      <input type="text" name="name" id="name" class="form-control" placeholder="" >
                                    </div>
                                    <div class="form-group my-3">
                                        <label for="emial">Email</label>
                                        <input type="text" name="email" id="email" class="form-control" placeholder="" >
                                      </div>
                                      <div class="form-group my-3">
                                        <label for="name">Password</label>
                                        <input type="password" name="password" id="password" class="form-control" placeholder="" >
                                      </div>
                                      
                                    <div class="form-group my-3">
                                        <label for="title">Bio</label>
                                        <textarea name="bio" id="bio" class="form-control" placeholder="" ></textarea>
                                      </div>
     
                                    <div class="form-group my-3">
                                        <label for="title">Profile Pic</label>
                                        <input accept=".png" type="file" name="profile" id="profile" class="form-control" placeholder="" >
                                      </div>
                                    <br>
                                   <div id="mytextarea" ></textarea>
                                    <button  class="btn-primary my-3">Create Author</button>
                                  </form>
                            </div>
                            
            
